function subStrUnderCond(s){
    let arr = s.split('');
    let temp_arr = [];
    
    for( i = 0; i < arr.length ; i++){
        let substr= '';
        for( j = i ; j < arr.length;j++){
            substr += arr[j];
            temp_arr.push(substr);
        }
    }

    let count =0;

    for(let  i = 0; i < temp_arr.length; i ++){
        if(temp_arr[i][0]==temp_arr[i][temp_arr[i].length-1])
            count++;
    }
    
    console.log(count);

}
